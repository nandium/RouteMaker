{
  "name": "yolo_bouldering",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint **/*.js --ignore-pattern node_modules/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,html,css,ts}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yarkhinephyo/yolo_bouldering.git"
  },
  "author": "yarkhinephyo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yarkhinephyo/yolo_bouldering/issues"
  },
  "homepage": "https://github.com/yarkhinephyo/yolo_bouldering#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  }
}
